{
  "name":              "puncoz/server-dashboard",
  "description":       "Dashboard and code-driven utilities for server related stuffs.",
  "keywords":          [
    "laravel",
    "server",
    "dashboard"
  ],
  "type":              "library",
  "license":           "MIT",
  "authors":           [
    {
      "name":  "Puncoz Nepal",
      "email": "info@puncoz.com"
    }
  ],
  "require":           {
    "php":                ">=7.1.0",
    "illuminate/support": "^5.6"
  },
  "require-dev":       {},
  "autoload":          {
    "psr-4": {
      "Puncoz\\ServerDashboard\\": "src/"
    }
  },
  "autoload-dev":      {
    "psr-4": {
      "Puncoz\\ServerDashboard\\Tests\\": "tests/"
    }
  },
  "extra":             {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel":      {
      "providers": [
        "Puncoz\\ServerDashboard\\ServerDashboardServiceProvider"
      ],
      "aliases":   {
        "ServerDashboard": "Puncoz\\ServerDashboard\\ServerDashboard"
      }
    }
  },
  "config":            {
    "sort-packages": true
  },
  "prefer-stable":     true,
  "minimum-stability": "dev"
}
